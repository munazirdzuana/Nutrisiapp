<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgrap"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/splasFragment"
        android:name="com.munaz.nutrisiapp.SplasFragment"
        android:label="fragment_splas"
        tools:layout="@layout/fragment_splas" >
        <action
            android:id="@+id/action_splasFragment_to_welcomeFragment"
            app:destination="@id/welcomeFragment"
            app:popUpTo="@id/splasFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.munaz.nutrisiapp.ui.login.loginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:enterAnim="@android:anim/slide_in_left"
            app:exitAnim="@android:anim/slide_out_right"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.munaz.nutrisiapp.ui.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@android:anim/slide_out_right"
            app:exitAnim="@android:anim/slide_in_left"
            app:popUpTo="@id/registerFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_registerFragment_to_identitasFragment"
            app:destination="@id/identitasFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.munaz.nutrisiapp.ui.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home"/>
    <fragment
        android:id="@+id/rekomenFragment"
        android:name="com.munaz.nutrisiapp.ui.rekomendMakan.rekomenFragment"
        android:label="fragment_rekomen"
        tools:layout="@layout/fragment_rekomen" />
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.munaz.nutrisiapp.ui.favMakanan.FavoriteFragment"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite"/>
    <fragment
        android:id="@+id/akunFragment"
        android:name="com.munaz.nutrisiapp.ui.akun.AkunFragment"
        android:label="fragment_akun"
        tools:layout="@layout/fragment_akun"/>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.munaz.nutrisiapp.ui.welcome.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_registerFragment"
            app:destination="@id/registerFragment"
            app:popUpTo="@id/registerFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_welcomeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/identitasFragment"
        android:name="com.munaz.nutrisiapp.ui.isidata.IdentitasFragment"
        android:label="fragment_identitas"
        tools:layout="@layout/fragment_identitas" >
        <action
            android:id="@+id/action_identitasFragment_to_budgetFragment"
            app:destination="@id/budgetFragment" />
    </fragment>
    <fragment
        android:id="@+id/budgetFragment"
        android:name="com.munaz.nutrisiapp.ui.isidata.BudgetFragment"
        android:label="fragment_budget"
        tools:layout="@layout/fragment_budget" >
        <action
            android:id="@+id/action_budgetFragment_to_riwayatFragment"
            app:destination="@id/riwayatFragment" />
    </fragment>
    <fragment
        android:id="@+id/riwayatFragment"
        android:name="com.munaz.nutrisiapp.ui.isidata.RiwayatFragment"
        android:label="fragment_riwayat"
        tools:layout="@layout/fragment_riwayat" >
        <action
            android:id="@+id/action_riwayatFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>